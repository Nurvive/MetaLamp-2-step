extends ../../templates/template
include ../../blocks/date-dropdown/date-dropdown
include ../../blocks/dropdown/dropdown
include ../../blocks/checkbox-buttons/checkbox-buttons
include ../../blocks/expandable-checkbox-list/expandable-checkbox-list
include ../../blocks/footer/footer
include ../../blocks/header/header
include ../../blocks/pagination/pagination
include ../../blocks/range-slider/range-slider
include ../../blocks/rich-checbox-buttons/rich-checbox-buttons
include ../../blocks/room-card/room-card

block title
    title search-room

block content
    .wrapper.search-room
        +header(false)
        main.search-room__main
            aside.search-room__aside
                ul.search-room__list
                    li.search-room__item.search-room__item_small-margin(data-move="no")
                        -
                            var options = [
                                {
                                    class: 'js-search-room__filter-date-dropdown_start',
                                    placeholder: 'ДД.ММ. - ДД.ММ.',
                                    size: 'long'
                                }
                            ]
                        .search-room__layout.search-room__dropdown-layout даты пребывания в отеле
                            +date-dropdown(options)
                    li.search-room__item(data-move="no")
                        .search-room__layout.search-room__dropdown-layout Гости
                            +dropdown('search-room__dropdown', 'default')
                    li.search-room__item(data-move="no")
                        .search-room__layout.search-room__slider-layout диапазон цены
                        +range-slider({
                            jsClassInput: 'js-search-room-slider__slider',
                            jsClassCounter: 'js-search-room-slider__count'
                        })
                        span.search-room__item-extra-text Стоимость за сутки пребывания в номере
                    li.search-room__item
                        .search-room__layout правила дома
                            +checkbox-buttons({items: ['Можно курить', 'Можно с питомцами', 'Можно пригласить гостей (до 10 человек)']})
                    li.search-room__item.search-room__item_small-margin
                        .search-room__layout доступность
                        +rich-checkbox-buttons({
                            'Широкий коридор': 'Ширина коридоров в номере не менее 91 см.',
                            'Помощник для инвалидов': 'На 1 этаже вас встретит специалист и проводит до номера.'
                        })

                    li.search-room__item
                        .search-room__layout.search-room__dropdown-layout удобства номера
                        +dropdown('search-room__dropdown', 'rooms')
                    li.search-room__item
                        .search-room__layout
                            +expandable-checkbox({
                                text: 'Дополнительные удобства',
                                items: ['Завтрак', 'Письменный стол', 'Стул для кормления', 'Кроватка', 'Телевизор', 'Шампунь', 'Телевизор', 'Шампунь'],
                                jsClass: 'js-expandable-checkbox__list'
                            })
                    ul.search-room__item.search-room__small-menu.small-menu
                        p.search-room__layout.small-menu-title(data-attr="small-menu-title") фильтр
                        .small-menu__content
