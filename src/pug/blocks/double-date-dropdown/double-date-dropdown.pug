mixin double-date-dropdown(obj)
    -var n = 1;
    include ../date-dropdown/date-dropdown
    each val in obj
        div.find-room__layout
            p.find-room__layout-text=val.text
            -var options = {id:val.idUnique, className:val.class}
            +date-dropdown(options)(id = val.id + n)
        -n++
