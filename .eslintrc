{
    "env": {
        "commonjs": true,
        "es2021": true,
        "jquery": true
    },
    "plugins": [
        "pug"
    ],
    "extends": [
        "airbnb-base/legacy"
    ],
    "parserOptions": {
        "sourceType": "module",
        "allowImportExportEverywhere": true,
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": [
            "error",
            4
        ],
        "no-underscore-dangle": [
            2,
            {
                "allow": [
                    "__filename",
                    "__dirname"
                ]
            }
        ],
        "func-names": [
            "error",
            "never"
        ],
        "object-curly-spacing": [
            "error",
            "never"
        ],
        "no-param-reassign": 0
    }
}
